{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Serverless Offline",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "debug"],
      "sourceMaps": true,
      "windows": {
        "type": "node",
        "request": "launch",
        "name": "Debug Serverless Offline",
        "runtimeArgs": ["run", "debug:win"],
      }
    }
  ]
}